	"tokenColors": [
		{
			"name": "Class",
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#C2AED0"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#E0AF91"
			}
		},
		{
			"name": "Function",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#C2AED0"
			}
		},
		{
			"name": "Function parameter",
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#E0AF91"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#DABFA5"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#E6C7AE"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#E0AF91"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#AFE091"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant.character",
				"constant.language",
				"constant.numeric",
				"constant.other",
				"support.constant"
			],
			"settings": {
				"foreground": "#AFE091"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#E0AF91"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"foreground": "#DABFA5"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#E0AF91"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#DABFA5"
			}
		},
		{
			"name": "Library API",
			"scope": [
				"support.function",
				"support.other.variable",
				"support.type",
			],
			"settings": {
				"foreground": "#E0AF91"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#AFE091"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#AFE091"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#C2AED0"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"comment.block.documentation.phpdoc.php",
				"entity.name.type.instance.jsdoc",
				"keyword.other.phpdoc.php",
				"keyword.other.type.php",
				"meta.other.type.phpdoc.php",
				"storage.type.class.jsdoc",
				"variable.other.jsdoc",
				"string.quoted.docstring.multi.python"
			],
			"settings": {
				"foreground": "#595959",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Method decorator",
			"scope": [
				"punctuation.definition.decorator.python",
				"entity.name.function.decorator.python",
				"meta.function.decorator.python",
				"meta.function.decorator.python support.type.python",
				"meta.function.decorator.python variable.parameter.function-call.python",
				"meta.function.decorator.python keyword.operator.assignment.python",
				"meta.function.decorator.python string.quoted.single.python",
				"meta.function.decorator.python constant.other.caps.python",
				"meta.function.decorator.python constant.language.python",
				"meta.function.decorator.python constant.numeric.float.python",
				"meta.function.decorator.python constant.numeric.dec.python"
			],
			"settings": {
				"foreground": "#595959",
				"fontStyle": "italic"
			}
		}
	]
}
